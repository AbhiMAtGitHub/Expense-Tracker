{
  "family": "ecs-task-def",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::245595379715:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::245595379715:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "containerDefinitions": [
    {
      "name": "expense-tracker-container",
      "image": "245595379715.dkr.ecr.ap-south-1.amazonaws.com/expense-tracker-repo",
      "cpu": 0,
      "portMappings": [
        {
          "name": "expense-tracker-port-tcp",
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ecs-task-def",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:5000/api/ping || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 10
      },
      "systemControls": []
    }
  ],
  "volumes": []
}
